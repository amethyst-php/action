{
    "name": "amethyst/action",
    "type": "library",
    "keywords": ["laravel", "amethyst", "action"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/relation": "0.2.*",
        "railken/template": "^1.0",
        "nicoswd/php-rule-parser": "^0.5.7",
        "guzzlehttp/guzzle": "^6.3",
        "namshi/cuzzle": "^2.0"
    },
    "require-dev": {
        "amethyst/foo": "0.2.*",
        "phpunit/phpunit": "7.*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "action",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\ActionServiceProvider"
            ]
        }
    }
}
